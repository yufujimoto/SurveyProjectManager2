<p id="notice"><%= notice %></p>

<h1>Projects</h1>

<table>
  <thead>
    <tr>
      <th>Uuid</th>
      <th>Name</th>
      <th>Title</th>
      <th>Begining</th>
      <th>Ending</th>
      <th>Phase</th>
      <th>Introduction</th>
      <th>Cause</th>
      <th>Created by</th>
      <th>Cover</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.uuid %></td>
        <td><%= project.name %></td>
        <td><%= project.title %></td>
        <td><%= project.begining %></td>
        <td><%= project.ending %></td>
        <td><%= project.phase %></td>
        <td><%= project.introduction %></td>
        <td><%= project.cause %></td>
        <td><%= project.created_by %></td>
        <td><%= project.cover %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path %>
